<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" prefHeight="158.0" prefWidth="135.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.question8.CounterController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Button fx:id="buttonUp" mnemonicParsing="false" onAction="#addTextField" text="Up" />
   <TextField fx:id="textFieldDisplay" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onInputMethodTextChanged="#addTextField" prefHeight="26.4" prefWidth="95.2" />
   <Button fx:id="buttonDown" mnemonicParsing="false" onAction="#minusTextField" text="Down" />
</VBox>
